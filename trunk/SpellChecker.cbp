			<Target title="wxSpellChecker">
				<Option output=".libs\libwxSpellChecker" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DHUNSPELL_STATIC" />
					<Add directory="wxspellchecker\include" />
					<Add directory="hunspell\src" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)\gcc_dll\mswu" />
				</Compiler>
				<Linker>
					<Add library="hunspell-1.2" />
					<Add library="wxmsw28u" />
					<Add directory="$(#wx.lib)\gcc_dll" />
				</Linker>
			</Target>
			<Target title="hunspell">
