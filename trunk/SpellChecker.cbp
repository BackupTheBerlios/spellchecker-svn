				</Compiler>
				<Linker>
					<Add library="wxmsw28u" />
					<Add directory="$(#wx.lib)\gcc_dll" />
				</Linker>
			</Target>
			<Target title="plugin">
				<Option output="SpellChecker" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-check-associations -ns -nd" />
				<Option host_application="$(#cb)\devel\codeblocks.exe" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DNO_CB_STATUS_BAR" />
					<Add directory="$(#cb)\include" />
					<Add directory="$(#cb)\include\wxscintilla\include" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)\gcc_dll\mswu" />
					<Add directory="wxspellchecker\include" />
					<Add directory="MyThes-1.0" />
				</Compiler>
				<Linker>
					<Add library="wxSpellChecker" />
					<Add library="hunspell-1.2" />
					<Add library="wxscintilla" />
					<Add library="codeblocks" />
					<Add library="wxmsw28u" />
					<Add directory=".libs" />
					<Add directory="$(#cb)\devel" />
					<Add directory="$(#wx.lib)\gcc_dll" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 SpellChecker.zip manifest.xml SpellCheckSettingsPanel.xrc" />
					<Add after="zip -j9 SpellChecker.cbplugin SpellChecker.dll SpellChecker.zip" />
					<Add after="zip -j9 SpellChecker.cbplugin SpellChecker.png SpellChecker-off.png" />
					<Add after='xcopy /Y SpellChecker.dll &quot;$(#cb)\devel\share\CodeBlocks\plugins\*&quot;' />
